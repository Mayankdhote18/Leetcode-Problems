class Solution {
public:
    string largestGoodInteger(string num) {
        string maxGood = "";

        for (int i = 0; i <= num.size() - 3; i++) {
            if (num[i] == num[i+1] && num[i+1] == num[i+2]) {
                string curr = num.substr(i, 3);
                if (curr > maxGood) {
                    maxGood = curr;
                }
            }
        }

        return maxGood;
    }
};
